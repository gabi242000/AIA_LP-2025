package tema2;
import java.io.*;
public class ex2 
{

	public static void main(String[] args) throws IOException
	{
		InputStreamReader isr = new InputStreamReader(System.in);
		BufferedReader br = new BufferedReader(isr);
		System.out.println("Introduceti un numar natural.");
		int nr = Integer.parseInt(br.readLine());
		int min = 9;
		int max = 0;
		if(nr == 0)
		{
			min = 0;
			System.out.println("Digit minim: " + min);
			System.out.println("Digit maxim: " + max);
		}
		while(nr > 0)
		{
			int c = nr % 10;
			if (c < min)
			{
				min = c;
			}
			if (c > max)
			{
				max = c;
			}
			nr = nr / 10;
		}
		System.out.println("Digit minim: " + min);
		System.out.println("Digit maxim: " + max);
	}

}
