ex1t4
import java.io*;
class Animal {
    String nume;
    String specie;
    int varsta;
    Animal() {
        nume = "necunoscut";
        specie = "necunoscuta";
        varsta = 0;
    }
   void afisareAnimal() {
        System.out.println("nume: " + nume);
        System.out.println("specie: " + specie);
        System.out.println("varsta: " + varsta);
    }
}
public class TestAnimal {
    public static void main(String[] args) {
        Animal a = new Animal();
        a.afisareAnimal();
    }
}
