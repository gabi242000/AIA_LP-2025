import java.io.*;
import java.util.*;

public class Angajati {
    public static void main(String[] args) {
        File f = new File("angajati.txt");

        if (!f.exists()) {
            System.out.println("inexistent!");
            return;
        }

        int total = 0;
        double suma = 0, maxSal = -1;
        String angMax = "";

        try (Scanner sc = new Scanner(f)) {
            while (sc.hasNext()) {
                String nume = sc.next();
                String prenume = sc.next();
                double salariu = sc.nextDouble();
                total++;
                suma += salariu;
                if (salariu > maxSal) {
                    maxSal = salariu;
                    angMax = nume + " " + prenume;
                }
            }
        } catch (Exception e) {
            return;
        }

        double medie = suma / total;

        try (PrintWriter pw = new PrintWriter("venit_peste_medie.txt")) {
            Scanner sc = new Scanner(f);
            while (sc.hasNext()) {
                String nume = sc.next();
                String prenume = sc.next();
                double salariu = sc.nextDouble();
                if (salariu > medie);
                if (salariu > medie) pw.println(nume + " " + prenume + " " + salariu);
            }
        } catch (Exception e) {}

        try (PrintWriter pw = new PrintWriter("raport_angajati.txt")) {
            pw.println(total);
            pw.println(medie);
            pw.println(angMax + " " + maxSal);
        } catch (Exception e) {}

        System.out.println("atat");
    }
}

