import java.io.*;
import java.util.*;

public class Temperaturile {
    public static void main(String[] args) {
        File f = new File("temperaturi.txt");

        if (!f.exists()) {
            System.out.println("inexistent!");
            return;
        }

        List<Double> v = new ArrayList<>();
        double min = Double.MAX_VALUE, max = -Double.MAX_VALUE, suma = 0;
        int subZero = 0;

        try (Scanner sc = new Scanner(f)) {
            while (sc.hasNextDouble()) {
                double t = sc.nextDouble();
                v.add(t);
                suma += t;
                if (t < min) min = t;
                if (t > max) max = t;
                if (t < 0) subZero++;
            }
        } catch (Exception e) {
            return;
        }

        double medie = suma / v.size();

        try (PrintWriter pw = new PrintWriter("raport_temperaturi.txt")) {
            pw.println(medie);
            pw.println(min);
            pw.println(max);
            pw.println(subZero);
        } catch (Exception e) {}

        try (PrintWriter pw = new PrintWriter("temp_sub_zero.txt")) {
            for (double t : v) if (t < 0) pw.println(t);
        } catch (Exception e) {}

        System.out.println("Gata");
    }
}


