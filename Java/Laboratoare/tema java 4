import java.util.Scanner;

public class Masina {
    String marca;
    String model;
    int an_fabricare;
    String culoare;
    double pret;

    public Masina(String marca, String model, int an_fabricare, String culoare, double pret) {
        this.marca = marca;
        this.model = model;
        this.an_fabricare = an_fabricare;
        this.culoare = culoare;
        this.pret = pret;
    }

    void afisareMasina() {
        System.out.println("marca: " + marca);
        System.out.println("model: " + model);
        System.out.println("an fabricatie: " + an_fabricare);
        System.out.println("culoare: " + culoare);
        System.out.println("pret: " + pret);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("brand cata valoare ");
        String marca = sc.nextLine();

        System.out.print("modelu?");
        String model = sc.nextLine();

        System.out.print("inainte sau dupa revolutie? (an?) ");
        int an = sc.nextInt();
        sc.nextLine();

        System.out.print("skin?");
        String culoare = sc.nextLine();

        System.out.print("cat e paguba?");
        double pret = sc.nextDouble();

        Masina m = new Masina(marca, model, an, culoare, pret);
        System.out.println("\nInfos");
        m.afisareMasina();

        sc.close();
    }
}

