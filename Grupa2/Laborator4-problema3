#include <stdio.h>

int main() {
    int N, i, j;
    
    printf("Introduceti dimensiunea matricei N: ");
    scanf("%d", &N);
    
    int matrice[N][N];

    for(i = 0; i < N; i++) {
        for(j = 0; j < N; j++) {
            if(j >= i) {
                matrice[i][j] = j + 1;
            } else {
                matrice[i][j] = matrice[j][i];
            }
        }
    }

    printf("Matricea generata:\n");
    for(i = 0; i < N; i++) {
        for(j = 0; j < N; j++) {
            printf("%d ", matrice[i][j]);
        }
        printf("\n");
    }

    return 0;
}
