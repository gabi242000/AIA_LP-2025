#include <stdio.h>

int main() {
    double salariu_brut, salariu_net;
    int cod;

    printf("Introduceti salariul brut: ");
    scanf("%lf", &salariu_brut);
    printf("Introduceti codul contractului (1, 2, 3): ");
    scanf("%d", &cod);

    switch (cod) {
        case 1: salariu_net = salariu_brut * 0.75; break; // 25% impozit
        case 2: salariu_net = salariu_brut * 0.90; break; // 10% impozit
        case 3: salariu_net = salariu_brut * 0.95; break; // 5% impozit
        default:
            printf("Cod invalid!\n");
            return 1;
    }

    printf("Salariul net: %.2lf lei\n", salariu_net);
    return 0;
}
