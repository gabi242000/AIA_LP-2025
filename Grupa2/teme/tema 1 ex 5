#include <iostream>

using namespace std;

int main() {
    int nota;
    cout << "Introduceti o nota (1-10): ";
    cin >> nota;
    
    if (nota == 10) cout << "Foarte bine";
    else if (nota == 9) cout << "Decent";
    else if (nota == 8) cout << "Bine";
    else if (nota == 7) cout << "Destul de ok";
    else if (nota == 6) cout << "OK";
    else if (nota == 5) cout << "Asa si asa";
    else if (nota == 4) cout << "Slabut";
    else if (nota == 3) cout << "slab";
    else if (nota == 2) cout << "Foarte slab";
    else if (nota == 1) cout << "Foarte foarte slab";
    else cout << "Nota invalida! Introduceti un numar intre 1 si 10.";
    
    cout << endl;
    return 0;
}
